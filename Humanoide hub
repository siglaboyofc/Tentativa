    Callback = function(Value)
        getgenv().ChaosHubAutoSpawnDoorbellValue = Value
        task.spawn(function()
            while getgenv().ChaosHubAutoSpawnDoorbellValue do
                if not SelectHouse then break end
                for _, v in pairs(workspace["001_Lots"]:GetChildren()) do
