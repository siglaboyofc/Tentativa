local function tween(obj, info, props)
    return TweenService:Create(obj, info, props)
end

-- Fade-in fundo
tween(bg, TweenInfo.new(0.5), {BackgroundTransparency = 0}):Play()
task.wait(0.6)

-- Entrada do logo
