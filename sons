    Name = "Rejoin Server",
    Callback = function()
    
    Notify(10709806740, "Espere.....", 3)
    
        local TeleportService = game:GetService("TeleportService")
        local Players = game:GetService("Players")

        TeleportService:Teleport(game.PlaceId, Players.LocalPlayer)
    end
})

local TabMain = Window:MakeTab({ "| Main", "Main" })

TabMain:AddButton({
    Name = "Tool Boombox (Universal Safe)",
    Callback = function()

        local Players = game:GetService("Players")
        local ReplicatedStorage = game:GetService("ReplicatedStorage")
        local player = Players.LocalPlayer
        local playerGui = player:WaitForChild("PlayerGui")

        local lastID = 142376088
        local gui

        -- Remove boombox antiga
        if player.Backpack:FindFirstChild("Boombox") then
            player.Backpack.Boombox:Destroy()
        end

        -- Tool
        local tool = Instance.new("Tool")
        tool.Name = "Boombox"
        tool.RequiresHandle = true
        tool.Parent = player.Backpack

        local handle = Instance.new("Part")
        handle.Name = "Handle"
        handle.Size = Vector3.new(1,1,1)
        handle.Transparency = 1
        handle.CanCollide = false
        handle.Parent = tool

        -- Procura qualquer Remote de som
        local function findSoundRemote()
            for _,v in ipairs(ReplicatedStorage:GetDescendants()) do
                if v:IsA("RemoteEvent") then
                    if string.find(v.Name:lower(), "sound")
                    or string.find(v.Name:lower(), "music")
                    or string.find(v.Name:lower(), "audio") then
                        return v
                    end
                end
            end
            return nil
        end

        local soundRemote = findSoundRemote()

        -- GUI
        local function createGui()
            gui = Instance.new("ScreenGui", playerGui)

            local frame = Instance.new("Frame", gui)
            frame.Size = UDim2.new(0.3,0,0.25,0)
            frame.Position = UDim2.new(0.35,0,0.35,0)
            frame.BackgroundColor3 = Color3.fromRGB(25,25,25)
            frame.Active = true
            frame.Draggable = true

            local box = Instance.new("TextBox", frame)
            box.Size = UDim2.new(1,-10,0.3,0)
            box.Position = UDim2.new(0,5,0.1,0)
            box.Text = tostring(lastID)
            box.TextScaled = true
            box.BackgroundColor3 = Color3.fromRGB(0,0,0)
            box.TextColor3 = Color3.new(1,1,1)

            local btn = Instance.new("TextButton", frame)
            btn.Size = UDim2.new(1,-10,0.3,0)
            btn.Position = UDim2.new(0,5,0.55,0)
            btn.Text = "PLAY"
            btn.TextScaled = true
            btn.BackgroundColor3 = Color3.fromRGB(40,40,40)
            btn.TextColor3 = Color3.new(1,1,1)

            btn.MouseButton1Click:Connect(function()
                local id = tonumber(box.Text)
                if not id then return end
                lastID = id

                -- FE (se existir remote)
                if soundRemote then
                    pcall(function()
                        soundRemote:FireServer(id)
                    end)
                end

                -- Local fallback (sempre funciona)
                local sound = Instance.new("Sound")
                sound.SoundId = "rbxassetid://"..id
                sound.Volume = 1
                sound.Parent = workspace
                sound:Play()

                task.delay(5,function()
                    sound:Destroy()
                end)

                gui:Destroy()
            end)
        end

        tool.Equipped:Connect(function()
            if not gui then
                createGui()
            end
        end)

        tool.Unequipped:Connect(function()
            if gui then
                gui:Destroy()
                gui = nil
            end
        end)
    end
})