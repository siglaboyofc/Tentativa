    TweenService:Create(desc, TweenInfo.new(0.3), {TextTransparency = 1}):Play()
    TweenService:Create(closeBtn, TweenInfo.new(0.3), {TextTransparency = 1}):Play()
    task.wait(0.6)
    gui:Destroy()
end)

wait(4)

