    TextTransparency = 0
}):Play()

-- Loop de cores do fundo
task.spawn(function()
    while frame.Parent do
        for hue = 0, 255, 2 do
            gradient.Color = ColorSequence.new{
                ColorSequenceKeypoint.new(0, Color3.fromHSV(hue/255, 1, 1)),
                ColorSequenceKeypoint.new(1, Color3.fromHSV((hue+85)/255, 1, 1))
            }
            task.wait(0.05)
        end
    end
end)

-- Efeito hover no bot√£o
